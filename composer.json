{
    "name": "andrey-vasin/laravel-request-filters",
    "description": "Simple & lean request filtering for Laravel 5.6+",
    "type": "library",
    "keywords": ["laravel", "filter", "request", "sanitize", "formrequest", "trait", "api", "rest", "utility"],
    "homepage": "https://github.com/andrey-vasin/laravel-request-filters",
    "require": {
        "illuminate/support": "*",
        "illuminate/contracts": "*",
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "AndreyVasin\\RequestFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreyVasin\\Tests\\": "test/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Wrightson",
            "email": "james.p.wrightson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
