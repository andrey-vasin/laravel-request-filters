{
    "name": "guerrilla/laravel-request-filters",
    "description": "Simple & lean request filtering for Laravel 5.6+",
    "type": "library",
    "keywords": ["laravel", "filter", "request", "sanitize", "formrequest", "trait", "api", "rest", "utility"],
    "homepage": "https://github.com/guerrillacontra/laravel-request-filters",
    "require": {
        "illuminate/support": "5.6 - ^9.0",
        "illuminate/contracts": "5.6 - ^9.0",
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "6 - ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Guerrilla\\RequestFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guerrilla\\Tests\\": "test/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Wrightson",
            "email": "james.p.wrightson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
